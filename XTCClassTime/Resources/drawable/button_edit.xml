<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="false">
    <shape android:shape="oval">
      <gradient android:angle="270" android:endColor="#007aff" android:startColor="#2dbaf7" android:type="linear" />
    </shape>
  </item>
  <item android:state_pressed="true">
    <shape android:shape="oval">
      <gradient android:angle="270" android:endColor="#b3007aff" android:startColor="#b32dbaf7" android:type="linear" />
    </shape>
  </item>
</selector>
