<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="false">    
    <shape android:shape="oval">
      <gradient android:angle="270" android:endColor="#ff3535" android:startColor="#ff6029" android:type="linear" />      
    </shape>
  </item>
  <item android:state_pressed="true">
    <shape android:shape="oval">
      <gradient android:angle="270" android:endColor="#b3ff3535" android:startColor="#b3ff6029" android:type="linear" />
    </shape>
  </item>
</selector>
