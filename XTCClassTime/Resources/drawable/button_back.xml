<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="false">
    <shape android:shape="oval">
      <gradient android:angle="270" android:endColor="#333333" android:startColor="#373737" android:type="linear" />
    </shape>
  </item>
  <item android:state_pressed="true">
    <shape android:shape="oval">
      <gradient android:angle="270" android:endColor="#4d333333" android:startColor="#4d373737" android:type="linear" />
    </shape>
  </item>
</selector>
